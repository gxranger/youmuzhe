.albumContentWrap {
    flex-wrap: wrap;
    justify-content: space-around;
    margin-top: 20rpx;
    padding: 0 15rpx;
}
.albumItem {
    width: 340rpx;
    height: 270rpx;
    margin: 10rpx 0;
}
.albumItem image {
    width: 100%;
    height: 100%;
    border-radius: 15rpx;
    box-shadow: 0 0 10rpx #747474;
}